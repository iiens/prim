{
  "name": "prim",
  "version": "3.1.2",
  "description": "Environment line production game",
  "main": "main.js",
  "author": "Legendary Games Studio",
  "license": "MIT",
  "repository": "https://github.com/lgs-games/prim",
  "keywords": [
    "prim",
    "lgs",
    "lgs-games"
  ],
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "distl": "electron-builder --linux deb tar.xz",
    "distl2": "electron-builder -l",
    "tsc": "npx tsc",
    "test": "mocha"
  },
  "build": {
    "appId": "com.lgs-games",
    "win": {
      "target": "nsis",
      "icon": "assets/img/primI.png"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "tar.gz"
      ],
      "icon": "assets/img/primI.png",
      "category": "Game",
      "maintainer": "Legendary Games Studio",
      "description": "Environment line production game"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "copyright": "Copyright Â© year ${author}"
  },
  "devDependencies": {
    "@electron/remote": "^1.2.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.2.2",
    "electron": "^12.0.13",
    "electron-builder": "^22.11.7",
    "mocha": "^9.0.2",
    "mocha-ts": "*",
    "ts-mocha": "^10.0.0"
  },
  "dependencies": {
    "package.json": "^2.0.1",
    "typescript": "^4.3.5",
    "nodemon": "^2.0.7",
    "npm": "^7.18.1"
  }
}
